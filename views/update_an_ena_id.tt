<!DOCTYPE html>
<html>

<script>

function goBack() 
 {
   window.history.back();
 }

function goHome()
 {
   window.location = '/';   
 }
</script>


<head>
<style>

P {
 font-size: 13px;
 font-family:'Courier New';
} 

.view_info input[type="submit"] {
 color: black;
 background: #BDECB6;
 display: inline;
}

table, th, td {
    border: 1px solid black;
    border-collapse: collapse;
}
th,td {
    padding: 15px;
    text-align: center;
}

</style>
</head>

<body background="images/zmp.jpg">
<div id="page">
      <div id="sidebar">
      </div>

      <div id="content">
        <div id="header">
         <p>
          <button onclick="goHome()">Home</button>
           &nbsp;&nbsp;&nbsp;&nbsp;
          <button onclick="goBack()">Back</button>
         <p>
         <h1>Choose an experiment</h1>
          <form action="<% update_an_ena_id_url %>" method="get">
           study_name :: experiment_name 
           &nbsp;&nbsp;&nbsp;&nbsp;
           <select class="dropdown" name="exp_to_check">
            <% FOREACH std_exp IN study_exp %>
             <% IF std_exp.0 == 'NullOption' %>
              <option value disabled selected> -- select an experiment -- </option>
             <% ELSE %>
              <% std_exp_res = std_exp.1 _ '::' _ std_exp.2 %>
              <option value="<% std_exp_res %>"><% std_exp.0 %> :: <% std_exp.1 %></option>
             <% END %>
            <% END %>
           </select></td>
           &nbsp;&nbsp;&nbsp;&nbsp;
           <input type="submit" value="Check">
          </form><p><p>
          <% FOREACH std_exp IN study_exp %>
           <% IF std_exp.0 != 'NullOption' %>
            &nbsp;&nbsp;
            <% std_exp.0 %> :: <% std_exp.1 %><p>
           <% END %>
          <% END %>
         <% IF ena_upd %>
          <table><tr>
          <% FOREACH row IN ena_upd %>
           <td><% row.1 %></td><td><% row.2 %></td></tr>
          <% END %>
         <% END %>
        </div>
      </div>
</div>
</body>
</html>
